{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "33ca41ca-cf15-488b-8bda-4700dcb5ecef",
        "SanitizeTinyMce": true
      },
      "Content": {
        "LoginBackgroundImage": "../cover.jpg",
        "LoginLogoImage": "../rocklogo.svg",
        "LoginLogoImageAlternative": "../cover.jpg",
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": false,
        "CacheBuster": "Version",
        "Version": "202310294"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "uSync": {
    "Settings": {
      "RootFolder": "uSync/v17/",
      "Folders": [
        "uSync/v17/"
      ],
      "ImportAtStartup": "All",
      "ExportOnSave": "All"
    }
  },
  "Smidge": {
    "debug": false,
    "fileWatchOptions": {
      "enabled": false
    },
    "Bundles": {
      "site-css": {
        "Files": [
          "~/css/core.min.css"
        ]
      },
      "site-js": {
        "Files": [
          "~/js/core.js"
        ]
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}